#include <stdio.h>

int fibonacci(int n) {
    long f=0;
    if(n==0){
        return 0;
    }else if (n==1){
        return 1;
    }else{
        long a=0;
        long b=1;
        for(int i=2; i<=n; i++){
            f=(long)(a+b);
            a=b;
            b=f;
        }
    }
    return f;
};

int main() {
    int n;
    scanf("%d", &n);
    printf("%ld", fibonacci(n));
    return 0;
}
