//int —>46—>1836311903

#include <stdio.h>

int fibonacci(int n) {
    int f=0;
    if(n==0){
        return 0;
    }else if (n==1){
        return 1;
    }else{
        int a=0;
        int b=1;
        for(int i=2; i<=n; i++){
            f=a+b;
            a=b;
            b=f;
        }
    }
    return f;
};

int main() {
    int n;
    scanf("%d", &n);
    printf("%d", fibonacci(n));
    return 0;
}
