//overflow CHAR --> 11 --> 89 

#include <iostream>

using namespace std;

int fibonacci(int n) {
    char f=0;
    if(n==0){
        return 0;
    }else if(n==1){
        return 1;
    }else{
        char a=0;
        char b=1;
        for(int i=2; i<=n; i++){
            f=(a+b);
            a=b;
            b=f;
        }
    }
    return f;
}
int main() {
    int n;
    cin >> n;
    cout << fibonacci(n);
    return 0;
}
